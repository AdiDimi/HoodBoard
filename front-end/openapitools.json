{
  "$schema": "node_modules/@openapitools/openapi-generator-cli/config.schema.json",
  "spaces": 2,
  "generator-cli": {
    "version": "7.10.0",
    "useDocker": true,
    "generators": {
      "v3.0": {
        "generatorName": "typescript-angular",
        "output": "src/app/models/generated",
        "inputSpec": "http://host.docker.internal:8080/swagger/v1/swagger.json",
        "additionalProperties": {
          "ngVersion": "20",
          "supportsES6": true,
          "npmName": "hood-board-api-client",
          "modelPackage": "models",
          "servicePackage": "services",
          "withInterfaces": true,
          "useSingleRequestParameter": true,
          "taggedUnions": false
        }
      }
    }
  }
}
